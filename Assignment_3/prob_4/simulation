#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5a1d27fab430 .scope module, "tb_gray_up_down_counter" "tb_gray_up_down_counter" 2 1;
 .timescale 0 0;
v0x5a1d27fc1970_0 .var "clk", 0 0;
v0x5a1d27fc1a30_0 .net "gray_code", 2 0, v0x5a1d27fc1630_0;  1 drivers
v0x5a1d27fc1b00_0 .var "reset", 0 0;
v0x5a1d27fc1c00_0 .var "up", 0 0;
S_0x5a1d27fab5c0 .scope module, "uut" "gray_up_down_counter" 2 9, 3 1 0, S_0x5a1d27fab430;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "up";
    .port_info 3 /OUTPUT 3 "gray_code";
v0x5a1d27fc1470_0 .var "binary_counter", 2 0;
v0x5a1d27fc1570_0 .net "clk", 0 0, v0x5a1d27fc1970_0;  1 drivers
v0x5a1d27fc1630_0 .var "gray_code", 2 0;
v0x5a1d27fc1720_0 .net "reset", 0 0, v0x5a1d27fc1b00_0;  1 drivers
v0x5a1d27fc17e0_0 .net "up", 0 0, v0x5a1d27fc1c00_0;  1 drivers
E_0x5a1d27f6fef0 .event posedge, v0x5a1d27fc1720_0, v0x5a1d27fc1570_0;
S_0x5a1d27f99670 .scope function.vec4.s3, "binary_to_gray" "binary_to_gray" 3 12, 3 12 0, S_0x5a1d27fab5c0;
 .timescale 0 0;
; Variable binary_to_gray is vec4 return value of scope S_0x5a1d27f99670
v0x5a1d27fc1390_0 .var "binary_value", 2 0;
TD_tb_gray_up_down_counter.uut.binary_to_gray ;
    %load/vec4 v0x5a1d27fc1390_0;
    %load/vec4 v0x5a1d27fc1390_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %xor;
    %ret/vec4 0, 0, 3;  Assign to binary_to_gray (store_vec4_to_lval)
    %end;
    .scope S_0x5a1d27fab5c0;
T_1 ;
    %wait E_0x5a1d27f6fef0;
    %load/vec4 v0x5a1d27fc1720_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x5a1d27fc1470_0, 0;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x5a1d27fc1630_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5a1d27fc17e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0x5a1d27fc1470_0;
    %cmpi/e 7, 0, 3;
    %jmp/0xz  T_1.4, 4;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x5a1d27fc1470_0, 0;
    %jmp T_1.5;
T_1.4 ;
    %load/vec4 v0x5a1d27fc1470_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x5a1d27fc1470_0, 0;
T_1.5 ;
    %load/vec4 v0x5a1d27fc1470_0;
    %store/vec4 v0x5a1d27fc1390_0, 0, 3;
    %callf/vec4 TD_tb_gray_up_down_counter.uut.binary_to_gray, S_0x5a1d27f99670;
    %assign/vec4 v0x5a1d27fc1630_0, 0;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5a1d27fab430;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5a1d27fc1970_0;
    %inv;
    %store/vec4 v0x5a1d27fc1970_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5a1d27fab430;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a1d27fc1970_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a1d27fc1b00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a1d27fc1c00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a1d27fc1b00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a1d27fc1b00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a1d27fc1c00_0, 0, 1;
    %delay 80, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a1d27fc1c00_0, 0, 1;
    %delay 30, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a1d27fc1c00_0, 0, 1;
    %delay 50, 0;
    %vpi_call 2 44 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5a1d27fab430;
T_4 ;
    %vpi_call 2 49 "$monitor", "Time = %0t | Reset = %b | UP = %b | Gray Code = %b", $time, v0x5a1d27fc1b00_0, v0x5a1d27fc1c00_0, v0x5a1d27fc1a30_0 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x5a1d27fab430;
T_5 ;
    %vpi_call 2 54 "$dumpfile", "tb_gray_up_down_counter.vcd" {0 0 0};
    %vpi_call 2 55 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5a1d27fab430 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    "gray.v";
