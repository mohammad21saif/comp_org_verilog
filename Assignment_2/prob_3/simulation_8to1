    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x56cc98397950;
T_1 ;
    %vpi_call 2 18 "$monitor", "Time = %0t | in = %b | sel = %b | out = %b", $time, v0x56cc983abd60_0, v0x56cc983abf20_0, v0x56cc983abe50_0 {0 0 0};
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x56cc983abd60_0, 0, 8;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x56cc983abf20_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x56cc983abf20_0, 0, 3;
