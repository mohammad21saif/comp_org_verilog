S_0x56cc98397950 .scope module, "tb_mux_8to1" "tb_mux_8to1" 2 1;
 .timescale 0 0;
v0x56cc983abd60_0 .var "in", 7 0;
v0x56cc983abe50_0 .net "out", 0 0, v0x56cc983abb60_0;  1 drivers
v0x56cc983abf20_0 .var "sel", 2 0;
S_0x56cc98397ae0 .scope module, "uut" "mux_8to1" 2 9, 3 1 0, S_0x56cc98397950;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "in";
    .port_info 1 /INPUT 3 "sel";
    .port_info 2 /OUTPUT 1 "out";
v0x56cc9835c760_0 .net "in", 7 0, v0x56cc983abd60_0;  1 drivers
v0x56cc983abb60_0 .var "out", 0 0;
v0x56cc983abc20_0 .net "sel", 2 0, v0x56cc983abf20_0;  1 drivers
E_0x56cc9835ce60 .event edge, v0x56cc983abc20_0, v0x56cc9835c760_0;
    .scope S_0x56cc98397ae0;
T_0 ;
    %wait E_0x56cc9835ce60;
    %load/vec4 v0x56cc983abc20_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 3;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 3;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
