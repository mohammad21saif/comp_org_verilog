    %cmp/u;
    %jmp/1 T_0.6, 6;
    %dup/vec4;
    %pushi/vec4 7, 0, 3;
    %cmp/u;
    %jmp/1 T_0.7, 6;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56cc983abb60_0, 0, 1;
    %jmp T_0.9;
T_0.0 ;
    %load/vec4 v0x56cc9835c760_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x56cc983abb60_0, 0, 1;
    %jmp T_0.9;
T_0.1 ;
    %load/vec4 v0x56cc9835c760_0;
    %parti/s 1, 1, 2;
    %store/vec4 v0x56cc983abb60_0, 0, 1;
    %jmp T_0.9;
T_0.2 ;
    %load/vec4 v0x56cc9835c760_0;
    %parti/s 1, 2, 3;
    %store/vec4 v0x56cc983abb60_0, 0, 1;
    %jmp T_0.9;
T_0.3 ;
