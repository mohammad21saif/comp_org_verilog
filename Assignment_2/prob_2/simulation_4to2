#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x641a2049c5c0 .scope module, "test_4to2" "test_4to2" 2 1;
 .timescale 0 0;
v0x641a204ae280_0 .var "a", 3 0;
v0x641a204ae340_0 .net "y", 1 0, v0x641a20464610_0;  1 drivers
S_0x641a2049c750 .scope module, "encoder" "encoder_4to2" 2 7, 3 1 0, S_0x641a2049c5c0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a";
    .port_info 1 /OUTPUT 2 "y";
v0x641a20464220_0 .net "a", 3 0, v0x641a204ae280_0;  1 drivers
v0x641a20464610_0 .var "y", 1 0;
E_0x641a20463310 .event edge, v0x641a20464220_0;
    .scope S_0x641a2049c750;
T_0 ;
    %wait E_0x641a20463310;
    %load/vec4 v0x641a20464220_0;
    %dup/vec4;
    %pushi/vec4 1, 0, 4;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 4;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 4;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 8, 0, 4;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 3, 3, 2;
    %store/vec4 v0x641a20464610_0, 0, 2;
    %jmp T_0.5;
T_0.0 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x641a20464610_0, 0, 2;
    %jmp T_0.5;
T_0.1 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x641a20464610_0, 0, 2;
    %jmp T_0.5;
T_0.2 ;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x641a20464610_0, 0, 2;
    %jmp T_0.5;
T_0.3 ;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x641a20464610_0, 0, 2;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x641a2049c5c0;
T_1 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x641a204ae280_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x641a204ae280_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x641a204ae280_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x641a204ae280_0, 0, 4;
    %delay 10, 0;
    %end;
    .thread T_1;
    .scope S_0x641a2049c5c0;
T_2 ;
    %vpi_call 2 26 "$monitor", "Time = %0t: a = %b, y = %b", $time, v0x641a204ae280_0, v0x641a204ae340_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    "4to2_encoder.v";
